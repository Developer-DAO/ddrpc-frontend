+}
+.button-primary:hover {
+	@apply hover:bg-blue-700;
+	box-shadow: none
+}
+
+.button-link {
+	@apply flex gap-3 w-fit px-5 py-3 bg-[#251E36] rounded-lg text-sm items-center;
+
+	box-shadow: rgba(16, 24, 40, 0.05) 0px 1px 2px 0px, 
+            	rgba(0, 0, 0, 0.25) 0px -2px 0px 0px inset, 
+            	rgba(255, 255, 255, 0.13) 0px 1px 0px 0px inset;
+	transition: all 0.2s ease;
+}
+.button-link-active {
+	@apply flex gap-3 w-fit px-5 py-3 bg-blue-600 rounded-lg text-sm items-center;
+
+	box-shadow: rgba(16, 24, 40, 0.05) 0px 1px 2px 0px, 
+            	rgba(0, 0, 0, 0.25) 0px -2px 0px 0px inset, 
+            	rgba(255, 255, 255, 0.13) 0px 1px 0px 0px inset;
+}
+.button-link:hover, .button-link-active:hover {
+	@apply hover:bg-blue-700;
+	box-shadow: none
+}
+
+.button-transparent {
+	@apply flex gap-3 w-fit px-5 py-3 bg-transparent rounded-lg text-sm items-center;
+}
+
+.button-outline {
+	@apply flex gap-3 w-fit px-5 py-3 rounded-lg text-sm border border-blue-800 hover:border-blue-600 items-center;	
+	transition: all 0.2s ease;
+}
+/* End Buttons */
+
+/* Inputs */
